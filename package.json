{
  "name": "notespace",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "concurrently \"npm run dev:client \" \"npm run dev:server \" \"npm run dev:css\" ",
    "build": "npm run build:css && npm run build:client && npm run build:server && cpx 'public/**/*' dist/public && cpx 'src/views/**/*' dist/src/views",
    "dev:server": "ts-node-dev --respawn ./src/index.ts",
    "dev:client": "npx webpack --mode=development --watch",
    "build:client": "npx webpack --mode=production",
    "build:server": "tsc",
    "dev:css": "npx tailwindcss -i ./public/css/global.css -o ./public/css/output.css --watch",
    "build:css": "npx tailwindcss -i ./public/css/global.css -o ./public/css/output.css --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/memcached": "^2.2.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.6",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "cpx": "^1.5.0",
    "css-loader": "^7.1.2",
    "express": "^4.21.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "choices.js": "^11.0.2",
    "concurrently": "^9.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "memcached": "^2.2.2",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-quill": "^2.0.0",
    "react-select": "^5.9.0",
    "react-toastify": "^10.0.6",
    "sequelize": "^6.37.4",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.8.0",
    "uuid": "^11.0.3",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  }
